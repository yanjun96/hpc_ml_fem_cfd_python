def retry(times):
    def decorator(func):
        def wrapper(*args, **kwargs):
            last_e = None
            for i in list( range(times) ):
                try:
                    return(func(*args, **kwargs))
                except Exception as e:
                    last_e = e
                    print(f'failed {i}:{e}')  
            raise last_e
        return wrapper
    return decorator

@retry(10)
def unstable():
    import random
    if random.random() < 0.7:
        raise ValueError('Bad luck')
    return "Success"
   
unstable()       



def uppercase(func):
    def wrapper(*args, **kwargs):
        c = func(*args, **kwargs)
        c_uppe = c.upper()
        return c_uppe
