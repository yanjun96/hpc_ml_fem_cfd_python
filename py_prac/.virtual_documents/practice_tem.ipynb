def requires_admin(func):
    def wrapper(*args, **kwargs):
        if kwargs.get('role') == 'admin':
            return func(*args, **kwargs)
        return "Access denied"
    return wrapper
 
@requires_admin
def delete_user(user_id, role = None):
    print(f"User {user_id} has deleted.")

