def log_call(func):
    def wrapper(*args, **kwargs):
        print(f'Calling {func.__name__} with arguments {args} and {kwargs}') 
        return func(*args, **kwargs)  
    return wrapper

@log_call
def add(a,b):
    return a+b

result = add(3,5)
print("Result:", result)





def measure_time(func):
    def wrapper(*args, **kwargs):
        import time
        start_t = time.time()
        result =  func(*args, **kwargs)
        end_t = time.time() - start_t
        print(f'The {func.__name__} function took {end_t} seconds.')
        return result
    return wrapper

@measure_time
def slow_function():
    import time
    time.sleep(0.1)
    return "Done"


print(slow_function())





def memoize(func):
    cache = {}
    
    def wrapper(*args):
        if args in cache:
            result = cache[args]
            print(f'do no need calculate fibonacci[{args}]')
            return result
        result = func(*args)
        cache[args] = result
        print(cache)    
        return result
    return wrapper

@memoize
def fibonacci(n):
    if n < 2:
        return n
    return fibonacci(n-1) + fibonacci(n-2)
            
