qsort(list)






class Polynomial:
    def __init__(self, coeff):
        self.coeff = coeff
        
    def __call__(self, x):
        s = 0
        for i in range(len(self.coeff)): s += self.coeff[i]*x**i
        return s

    def __call1__(self, x):
        return x**2


p = Polynomial([1,2,3])


p.__call1__(3)


