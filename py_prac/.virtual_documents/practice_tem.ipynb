    def __init__(self, charge, mass, position,flavor):
        self.c = charge
        self.m = mass
        self.r = position
        self.flavor = flavor


    
    def hear(self):
        myroar = self.roar +(
            '\nMy charge is '+str(self.c) +
            '\nMe mass is '+ str(self.m))
        print(myroar)

    def flip(self):
        if self.flavor == "up":
           self.flavor = "down"
        elif self.flavor == 'down':
           self.flavor = 'up'
        return self.flavor

    # Without this @staticmethod, use c1.possible can't return.
    @staticmethod 
    def possible():
        return ['up','down','top']



